{
    "name": "recras/symfony1",
    "description": "Fork of symfony 1.4, based on lexpress/symfony1",
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.7.15",
        "psr/log": "*"
    },
    "type": "library",
    "autoload": {
        "classmap": [
            "lib/validator"
        ],
        "files": [
            "autoload.php"
        ]
    },
    "suggest": {
        "lexpress/doctrine1": "Doctrine plugin",
        "propel/sf-propel-o-r-m-plugin": "Propel plugin",
        "psr/log": "Psr logger"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "bin": [
        "data/bin/symfony"
    ]
}
